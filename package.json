    "@iconify-json/mdi": "^1.1.47",
    "@iconify-json/pepicons": "^1.1.12",
    "@iconify-json/ph": "^1.1.4",
    "@iconify-json/uil": "^1.1.4",
    "@sveltejs/kit": "^2.5.6",
    "@sveltejs/vite-plugin-svelte": "^3.1.0",
    "autoprefixer": "^10.4.13",
    "onchange": "7.1.0",
    "postcss": "^8.4.21",
