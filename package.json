  "type": "module",
  "scripts": {
    "dev": "vite --host 0.0.0.0 --port 4040",
    "build": "vite build",
    "clean": "rm -rf node_modules dist",
    "serve": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
