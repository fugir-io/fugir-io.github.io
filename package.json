{
  "name": "fugir-io",
  "version": "13.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host 0.0.0.0 --port 4040",
    "build": "tsc && vite build",
    "clean": "rm -rf node_modules dist",
    "serve": "vite preview",
    "check": "tsc --noEmit",
    "check:watch": "tsc --noEmit --watch",
    "lint": "prettier --check . && eslint .",
    "format-watch": "onchange 'src/**/*.{ts,tsx}' -- prettier --write {{changed}}",
    "format": "prettier --write .",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "ci": "npm run lint && npm run check && npm run test:run && npm run build",
    "test:local": "./scripts/test-local.sh",
    "dev:watch": "./scripts/test-watch.sh",
    "coverage": "./scripts/coverage-report.sh",
    "start": "npm run dev"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@iconify-json/gg": "^1.1.4",
    "@iconify-json/ic": "^1.1.13",
    "@iconify-json/icon-park-outline": "^1.1.10",
    "@iconify-json/ion": "^1.1.8",
    "@iconify-json/material-symbols": "^1.1.78",
    "@iconify-json/mdi": "^1.1.47",
    "@iconify-json/pepicons": "^1.1.12",
    "@iconify-json/ph": "^1.1.4",
    "@iconify-json/uil": "^1.1.4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/eslint__js": "^8.42.3",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.29.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "jsdom": "^26.1.0",
    "onchange": "7.1.0",
    "postcss": "^8.4.21",
    "prettier": "^3.1.1",
    "sass": "^1.75.0",
    "typescript": "^5.4.5",
    "typescript-eslint": "^8.35.0",
    "vite": "^5.2.9",
    "vite-imagetools": "7.0.1",
    "vite-plugin-pwa": "^0.19.8",
    "vitest": "^3.2.4",
    "workbox-window": "^7.0.0"
  },
  "dependencies": {
    "@auth0/auth0-spa-js": "^1.22.0",
    "@fontsource/inter": "^5.0.17",
    "@neodrag/vanilla": "^2.0.6",
    "axios": "1.6.8",
    "date-fns": "^3.6.0",
    "framer-motion": "^11.2.12",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.24.0",
    "unplugin-icons": "^0.18.5",
    "use-sound": "^4.0.1",
    "zustand": "^4.5.2"
  }
}
